<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.diagnostics>
    <sources>
      <source name="IntactVisionServer.Scripting" switchValue="All">
        <listeners>
          <add name="delimfile" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="delimfile" type="System.Diagnostics.DelimitedListTraceListener"
           delimiter=";" initializeData="E:\LogFile\IntactVisionServer_Scripting.csv" traceOutputOptions="ProcessId, DateTime" />
    </sharedListeners>
  </system.diagnostics>
  <appSettings>
    <add key="server_ip" value="localhost"/>
  </appSettings>
</configuration>